<html lang="en">
  <body>
  
    <p>
      <strong>Name:</strong>
      <br />
      <%= @contact.name %>
    </p>
    
    
    <p>
      <strong>Telephone:</strong>
      <br />
      <%= @contact.telephone %>
    </p>    
    
    
    <p>
      <strong>Email:</strong>
      <br />
      <%= mail_to(@contact.email) %>
    </p>
    
    
    <p>
      <strong>Building Name:</strong>
      <br />
      <%= @contact.building_name %>
    </p>    
    
    
    <p>
      <strong>Message:</strong>
      <br />
      <%= @contact.message %>
    </p>
    
  </body>
</html>
