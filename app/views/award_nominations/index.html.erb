<h1>Listing award_nominations</h1>

<table>
  <tr>
    <th>Award</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @award_nominations.each do |award_nomination| %>
  <tr>
    <td><%= award_nomination.award %></td>
    <td><%= award_nomination.user %></td>
    <td><%= link_to 'Show', award_nomination %></td>
    <td><%= link_to 'Edit', edit_award_nomination_path(award_nomination) %></td>
    <td><%= link_to 'Destroy', award_nomination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Award nomination', new_award_nomination_path %>
